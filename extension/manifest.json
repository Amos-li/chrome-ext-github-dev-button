{
  "manifest_version": 2,
  "name": "GitHub Dev Button",
  "description": "Adds a github.dev button to GitHub repo pages",
  "version": "0.0.1",
  "homepage_url":
    "https://github.com/amos-li/chrome-ext-github-tweet-button",
  "author": "Amos",
  "permissions": ["tabs", "*://*.github.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["github-tweet-button.js"]
    }
  ],
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
